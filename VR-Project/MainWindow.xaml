<Window x:Class="VR_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VR_Project"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="button" Content="Button" Command="{Binding Path=SelectEngine}"/>
            <ComboBox Width="300" Height="100" ItemsSource="{Binding ob}" DisplayMemberPath="clientinfo.host" SelectedItem="{Binding SelectClient}"></ComboBox>
            <Button x:Name="connectToServer" Content="Connect to Server" Command="{Binding Path=ConnectToServer}"/>

        </StackPanel>
    </Grid>
</Window>
